<resources>


    <style name="matchmatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">@dimen/common_margin_medium</item>
    </style>

    <style name="matchmatch.margin">
        <item name="android:layout_margin">@dimen/common_margin_bigger</item>
    </style>

    <style name="matchmatch.marginhuge">
        <item name="android:layout_margin">@dimen/common_margin_huge</item>
        <item name="android:padding">@dimen/common_padding_medium</item>
    </style>

    <style name="matchmatch.listviewgeneral">
        <item name="android:divider">@color/selector</item>
        <item name="android:dividerHeight">@dimen/common_divider_high</item>
    </style>

    <style name="matchwrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="matchwrap.margin">
        <item name="android:layout_margin">@dimen/common_margin_medium</item>
    </style>

    <style name="matchmatch.input">
        <item name="android:textSize">@dimen/common_textsize_medium</item>
    </style>

    <style name="matchwrap.input">
        <item name="android:textSize">@dimen/common_textsize_medium</item>
    </style>

    <style name="matchwrap.biginput">
        <item name="android:textSize">@dimen/common_textsize_big</item>
        <item name="android:layout_margin">@dimen/common_margin_medium</item>
        <item name="android:minHeight">@dimen/common_custom_height</item>
        <item name="android:padding">@dimen/common_padding_medium</item>
    </style>

    <style name="matchwrap.biginput.title">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="matchwrap.input.area">
        <item name="android:inputType">textMultiLine</item>
        <item name="android:minLines">4</item>
        <item name="android:singleLine">false</item>
    </style>

    <style name="matchwrap.input.title">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="wrapwrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wrapwrap.margin">
        <item name="android:layout_margin">@dimen/common_margin_medium</item>
    </style>

    <style name="wrapwrap.marginbiggerST">
        <item name="android:layout_marginStart">@dimen/common_margin_bigger</item>
        <item name="android:layout_marginTop">@dimen/common_margin_bigger</item>
    </style>

    <style name="wrapwrap.marginbiggerET">
        <item name="android:layout_marginEnd">@dimen/common_margin_bigger</item>
        <item name="android:layout_marginTop">@dimen/common_margin_bigger</item>
    </style>

    <style name="wrapwrap.input">
        <item name="android:textSize">@dimen/common_textsize_medium</item>
    </style>

    <style name="wrapwrap.title">
        <item name="android:padding">@dimen/common_padding_medium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_margin">@dimen/common_margin_big</item>
        <item name="android:textSize">@dimen/common_textsize_big</item>
    </style>

    <style name="wrapmatch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="custom_wrap">
        <item name="android:layout_width">@dimen/common_custom_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/common_textsize_big</item>
        <item name="android:layout_margin">@dimen/common_margin_medium</item>
        <item name="android:minHeight">@dimen/common_custom_height</item>
    </style>

    <style name="custom_wrap.title">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="custom_cell_keypad">
        <item name="android:layout_width">@dimen/big_circle_diameter</item>
        <item name="android:layout_height">@dimen/big_circle_diameter</item>
        <item name="android:textSize">@dimen/common_textsize_big</item>
        <item name="android:padding">@dimen/common_margin_big</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_margin">@dimen/common_margin_medium</item>
    </style>

    <style name="custom_cell_keypad.smaller">
        <item name="android:layout_width">@dimen/regular_circle_diameter</item>
        <item name="android:layout_height">@dimen/regular_circle_diameter</item>
    </style>
<!--
    <style name="spinner_custom" parent="Widget.AppCompat.Spinner.Underlined">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/common_margin_medium</item>
        <item name="android:padding">@dimen/common_padding_medium</item>
    </style>
-->
    <style name="spinner_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/common_margin_medium</item>
        <item name="android:padding">@dimen/common_padding_medium</item>
    </style>

    <style name="ctm_btn_pad_num">
        <item name="android:layout_height">@dimen/side_btn_pad_num</item>
        <item name="android:layout_width">@dimen/side_btn_pad_num</item>
        <item name="android:layout_margin">@dimen/common_margin_small</item>
        <item name="android:background">@drawable/style_rectangle</item>
        <item name="android:textSize">@dimen/common_textsize_biggest</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="dialog_component">
        <item name="android:layout_width">@dimen/dialog_width</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="dialog_component.title">
        <item name="android:layout_margin">@dimen/common_margin_medium</item>
    </style>

    <style name="white_check">
        <item name="colorAccent">#fff</item>
    </style>
</resources>
